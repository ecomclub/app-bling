[
  {
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "synchronize"
    ],
    "properties": {
      "synchronize": {
        "$id": "#/properties/synchronize",
        "type": "object",
        "title": "The Synchronize Schema",
        "required": [
          "ecomplus",
          "bling"
        ],
        "properties": {
          "ecomplus": {
            "$id": "#/properties/synchronize/properties/ecomplus",
            "type": "object",
            "title": "The Ecomplus Schema",
            "required": [
              "products",
              "orders"
            ],
            "properties": {
              "products": {
                "$id": "#/properties/synchronize/properties/ecomplus/properties/products",
                "type": "object",
                "title": "The Products Schema",
                "required": [
                  "all",
                  "auto",
                  "ids"
                ],
                "properties": {
                  "all": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/products/properties/all",
                    "type": "boolean",
                    "title": "The All Schema",
                    "default": false,
                    "examples": [
                      true
                    ]
                  },
                  "auto": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/products/properties/auto",
                    "type": "boolean",
                    "title": "The Auto Schema",
                    "default": false,
                    "examples": [
                      false
                    ]
                  },
                  "ids": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/products/properties/ids",
                    "type": "array",
                    "title": "The Ids Schema"
                  }
                }
              },
              "orders": {
                "$id": "#/properties/synchronize/properties/ecomplus/properties/orders",
                "type": "object",
                "title": "The Orders Schema",
                "required": [
                  "all",
                  "auto",
                  "ids"
                ],
                "properties": {
                  "all": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/orders/properties/all",
                    "type": "boolean",
                    "title": "The All Schema",
                    "default": false,
                    "examples": [
                      true
                    ]
                  },
                  "auto": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/orders/properties/auto",
                    "type": "boolean",
                    "title": "The Auto Schema",
                    "default": false,
                    "examples": [
                      false
                    ]
                  },
                  "ids": {
                    "$id": "#/properties/synchronize/properties/ecomplus/properties/orders/properties/ids",
                    "type": "array",
                    "title": "The Ids Schema"
                  }
                }
              }
            }
          },
          "bling": {
            "$id": "#/properties/synchronize/properties/bling",
            "type": "object",
            "title": "The Bling Schema",
            "required": [
              "orders",
              "products"
            ],
            "properties": {
              "orders": {
                "$id": "#/properties/synchronize/properties/bling/properties/orders",
                "type": "object",
                "title": "The Orders Schema",
                "required": [
                  "synchronize"
                ],
                "properties": {
                  "synchronize": {
                    "$id": "#/properties/synchronize/properties/bling/properties/orders/properties/synchronize",
                    "type": "boolean",
                    "title": "The Synchronize Schema",
                    "default": false,
                    "examples": [
                      false
                    ]
                  }
                }
              },
              "products": {
                "$id": "#/properties/synchronize/properties/bling/properties/products",
                "type": "object",
                "title": "The Products Schema",
                "required": [
                  "stock"
                ],
                "properties": {
                  "stock": {
                    "$id": "#/properties/synchronize/properties/bling/properties/products/properties/stock",
                    "type": "boolean",
                    "title": "The Stock Schema",
                    "default": false,
                    "examples": [
                      true
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  {
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "hidden_data"
    ],
    "properties": {
      "hidden_data": {
        "$id": "#/properties/hidden_data",
        "type": "object",
        "title": "The Hidden_data Schema",
        "required": [
          "bling_api_key",
          "bling_loja_id"
        ],
        "properties": {
          "bling_api_key": {
            "$id": "#/properties/hidden_data/properties/bling_api_key",
            "type": "string",
            "title": "The Bling_api_key Schema",
            "pattern": "^(.*)$"
          },
          "bling_loja_id": {
            "$id": "#/properties/hidden_data/properties/bling_loja_id",
            "type": "integer",
            "title": "The Bling_loja_id Schema"
          }
        }
      }
    }
  }
]